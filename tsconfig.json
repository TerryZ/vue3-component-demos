{
  "compilerOptions": {
    "outDir": "./built",
    "target": "es5",
    "strict": true,
    "module": "es2015",
    "moduleResolution": "node",
    "allowJs": true,
    "jsx": "preserve",
    "paths": {
      "@/*": ["./src/*"],
      "@view/*": ["./src/views/*"],
      "@http/*": ["./src/config/http/*"]
    }
  }
}
